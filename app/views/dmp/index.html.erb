<html>
<head>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tag %>
</head>
<body>
  <%= link_to 'login', new_dmp_admin_session_path %><br>
  <hr>
  <% @dmp_requests.each do |dmp_request| %>
    <%= link_to dmp_request.name, dmp_show_path(dmp_request.id) %> 
    <font size="1">
      <%= link_to "Edit", dmp_edit_path(dmp_request.id) if dmp_admin_signed_in? %> 
      <%= link_to "Destroy", dmp_destroy_path(dmp_request.id) if dmp_admin_signed_in? %>
    </font><br>
  <% end %>
  <hr>
  <% if dmp_admin_signed_in? %>
    <%= link_to "Новый запрос", dmp_new_path %>
  <% end %>
</body>
</html>